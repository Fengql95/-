   "source": [
    "from PIL import ImageGrab\n",
    "import time\n",
    " \n",
    "count = 0 \n",
    "while True:\n",
    "    count += 1\n",
    "    pic = ImageGrab.grab()\n",
    "    name = time.strftime(\"%Y-%m-%d---%H:%M:%S\", time.localtime()).replace(\":\", \"-\")\n",
    "    pic.save('D:\\每日截屏\\%s.jpg' %  name)\n",
    "    print(count)\n",
    "    time.sleep(600)"
   ]
